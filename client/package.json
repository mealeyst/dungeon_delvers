{
  "name": "@dungeon_delvers/client",
  "version": "0.0.1",
  "description": "The Front End 3D Client for Dungeon Delvers powered by BabylonJS",
  "homepage": "./",
  "directories": {
    "buildResources": "dist",
    "app": "dist"
  },
  "scripts": {
    "build": "parcel build ./public/index.html --public-url ./",
    "clean": "rm -rf .parcel-cache dist",
    "deploy-demo": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "start": "parcel --open chrome ./public/index.html",
    "test": "jest"
  },
  "keywords": [
    "BabylonJS",
    "Browser",
    "Game",
    "Javascript",
    "Typescript",
    "3D"
  ],
  "author": "Shawn Mealey",
  "license": "ISC",
  "dependencies": {
    "@babylonjs/core": "^6.8.0",
    "@babylonjs/gui": "^6.12.3",
    "@babylonjs/inspector": "^6.8.0",
    "@parcel/types": "^2.9.2",
    "delaunay-triangulate": "^1.1.6",
    "earcut": "^2.2.4",
    "parcel": "^2.9.2"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.9.2",
    "@parcel/core": "^2.9.2",
    "@types/earcut": "^2.1.1"
  }
}
